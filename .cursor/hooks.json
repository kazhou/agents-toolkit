{
  "version": 1,
  "hooks": {
    "sessionStart": [
      {
        "command": "./.cursor/hooks/init-agent-logs.sh",
        "timeout": 5
      }
    ],
    "afterFileEdit": [
      {
        "command": "./.cursor/hooks/save-planning-logs-cursor.sh",
        "matcher": "**/plans/*.md",
        "timeout": 30
      }
    ],
    "sessionEnd": [
      {
        "command": "./.cursor/hooks/save-planning-logs-cursor.sh",
        "timeout": 30
      }
    ]
  }
}
